# TeleMind Bot Environment Variables
# Rename this file to .env and fill in your values

# API Keys
GROQ_API_KEY=your_groq_api_key_here
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Deployment
WEBHOOK_URL=https://your-deployment-url.com
PORT=8000

# Optional Configuration
# Uncomment and set if needed
# FIREBASE_SERVICE_ACCOUNT={"type":"service_account",...}  # JSON string if not using file
# TESSERACT_PATH=/path/to/tesseract  # Only needed if default path doesn't work
# DEBUG=True  # Set to True for verbose logging
